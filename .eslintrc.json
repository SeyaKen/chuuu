{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-use-before-define": 0,
        "linebreak-style": 0,
        "global-require": 0,
        "react/prop-types": ["error", { "ignore":["navigation"]}],
        "max-len": 0,
        "quotes": 0,
        "no-console": 0,
        "prefer-destructuring": 0,
        "no-empty": 0,
        "no-plusplus": 0,
        "no-else-return": 0,
        "arrow-body-style": 0,
        "no-trailing-spaces": 0
    }
}
